   "source": [
    "# Given: Two positive integers a and b (a<b<10000).\n",
    "# Return: The sum of all odd integers from a through b, inclusively.\n",
    "\n",
    "a = int(input(\"Enter number:\"))\n",
    "b = int(input(\"Enter another number:\"))\n",
    "sum = 0\n",
    "for i in range(a,b+1):\n",
    "    if i % 2 == 1:\n",
    "        sum+=i\n",
    "print(sum)\n",
    "        "
   ]
  }
